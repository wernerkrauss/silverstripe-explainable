	"type": "silverstripe-module",
	"keywords": ["silverstripe", "accessibility"],
	"license": "MIT",
	"authors": [
	{
		"name": "Werner Krau√ü",
		"email": "werner.krauss@netwerkstatt.at",
		"homepage": "https://www.netwerkstatt.at",
		"role": "Developer"
	}
	],
	"support": {
		"issues": "https://github.com/wernerkrauss/silverstripe-explainable/issues"
	},
